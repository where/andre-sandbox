<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <bean id="_keyValueDao" class="com.amm.nosql.dao.cassandra.astyanax.AstyanaxKeyValueDao" >
    <constructor-arg value="${cassandra.cluster}"/>
    <constructor-arg value="${cassandra.keyspace.keyValue}"/>
    <constructor-arg value="${cassandra.columnFamily.keyValue}"/>
    <constructor-arg value="${astyanax.seedUrl}"/>
    <constructor-arg value="${astyanax.port}"/>
    <constructor-arg ref="keyValueMapper" />
  </bean>

  <bean id="keyValueDao" class="com.amm.nosql.dao.cassandra.astyanax.AstyanaxKeyValueStringDao" >
    <constructor-arg value="${cassandra.cluster}"/>
    <constructor-arg value="${cassandra.keyspace.keyValue}"/>
<!--
    <constructor-arg value="${cassandra.columnFamily.keyValue.string}"/>
-->
    <constructor-arg value="KeyValue2" />
    <constructor-arg value="${astyanax.seedUrl}"/>
    <constructor-arg value="${astyanax.port}"/>
    <constructor-arg ref="keyValueMapper" />
  </bean>

</beans>
